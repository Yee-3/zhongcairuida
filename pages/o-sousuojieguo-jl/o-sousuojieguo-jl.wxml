			<view class="top_f">
				<image src="../img/f047.png"></image>
			</view>
			<view class="top_g" bindtap="weizhi">
				<p>{{mapValue}}</p>
			</view>
			<view class="top_h">
				<image src="../img/f048.png"></image>
			</view>
		</view>
		<view class="top_c">
			<view class="top_e">
				<image src="../img/f057.png"></image>
			</view>
