<view class="q_01">
	<view class="q_02">
		<view class="k_01">
			<image src="../img/f0666.jpg"></image>
		</view>
		<view class="k_02">
			<view class="fl">
				<view class="q_03">
					<view class="q_04">{{user.name?user.name:'暂无用户名'}}</view>
				</view>
				<view class="q_06">
					<view class="k_03 fl">
						<image src="../img/q005.png"></image>
					</view>
					<view class="q_08 fl">{{user.companyName?user.companyName:'暂无公司信息'}}</view>
				</view>
				<view class="q_06">
					<view class="k_04 fl">
						<image src="../img/f024.png"></image>
					</view>
					<view class="q_08 fl">{{user.phone?user.phone:''}}</view>
				</view>
				<view class="q_06">
					<view class="k_05 fl">
						<image src="../img/f025.png"></image>
					</view>
					<view class="q_08 fl">{{user.email?user.email:''}}</view>
